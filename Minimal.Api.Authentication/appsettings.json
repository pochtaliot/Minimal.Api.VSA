{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=UsersApiDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Authentication": {
    "LoginAttemptsLimit": 5,
    "Schemes": {
      "Bearer": {
        "Active": true,
        "ValidAudience": "http://abc.com",
        "ValidIssuer": "http://abc.com",
        "TokenExpirationMinutes": 30
      }
    }
  }
}
